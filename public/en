        <script>
            var urlParams = new URLSearchParams(window.location.search);
            if (urlParams.has('shipmentNumber')) {
                var sn = urlParams.get('shipmentNumber');
                if (sn.startsWith('000') && sn.length == 14) {
                    urlParams.set('shipmentNumber', sn.slice(3));
                    history.pushState(null, null, "?" + urlParams.toString());
                }
            }
            var shipNumber = document.getElementById('shipmentNumber');
            shipNumber.addEventListener('input', function() {
                var input = this.value;
                if (input.startsWith('000') && input.length == 14) {
                    this.value = input.slice(3);
                }
            });
        </script>
