    <script type="text/javascript">
        var searchNumber = document.getElementsByClassName('serachNumber');
        for (var i = 0; i < searchNumber.length; i++) {
            searchNumber[i].addEventListener('input', function () {
                var input = this.value;
                if (input.startsWith('000') && input.length == 14) {
                    this.value = input.slice(3);
                }
            });
        }
        document.addEventListener("buildJsLoaded", function () {
            $('#searchForm').on('submit', function (e) {
                $search = $('#search');
                if ($search.val().length < 3) {
                    e.preventDefault();
                }
            });
        });
    </script>
    <script type="text/javascript">
        document.addEventListener("buildJsLoaded", function () {
            var timeout = false;
            var ajax = false;
            var $searchInput = $("#search");
            var ajaxCache = {};
            $searchInput.on("keyup", function () {
                if ($(this).val().length >= 3) {
                    if (timeout) {
                        clearTimeout(timeout);
                        timeout = false;
                    }
                    if (ajax) {
                        ajax.abort();
                    }
                    var val = $searchInput.val();
                    if (val in ajaxCache) {
                        $("#results").show().html(ajaxCache[val]);
                    } else {
                        timeout = setTimeout(function () {
                            ajax = $.ajax({
                                url: '/bg/search',
                                data: {
                                    q: val
                                }
                            }).done(function (response) {
                                $("#results").show().html(response.html);
                                ajaxCache[val] = response.html;
                                timeout = false;
                                ajax = false;
                            });
                        }, 200);
                    }
                }
            });
        });
    </script>

