        <script>
        function buildJsReady() {
            if (typeof window.CustomEvent !== 'function') {
                function CustomEvent(event, params) {
                    params = params || {
                        bubbles: false,
                        cancelable: false,
                        detail: undefined
                    };
                    var evt = document.createEvent('CustomEvent');
                    evt.initCustomEvent(event, params.bubbles, params.cancelable, params.detail);
                    return evt;
                }
                CustomEvent.prototype = window.Event.prototype;
                window.CustomEvent = CustomEvent;
            }
            var event = new window.CustomEvent('buildJsLoaded');
            document.dispatchEvent(event);
        }
    </script>
    <script onload="buildJsReady();" src="public/assets/js/build.js"></script>

    <script src="public/dist/js/jquery-cookie.js"></script>
    <!-- <script src="public/dist/js/cookies_main.js"></script>-->
    <script type="text/javascript">
        var email;
        var $request;
        $(function () {
            var $newsletterForm = $("#newsletter");
            var $bullteineGdprCheck = $newsletterForm.find(".validation-checkbox");
            var validateGdpr = function () {
                var isValid = true;
                $bullteineGdprCheck.each(function (index, el) {
                    if ($(el).data("required") === true) {
                        if (!$(el).is(":checked")) {
                            $(el).addClass("validation-failed");
                            $(el).removeClass("validation-passed");
                            var errorMessage = $(el).data("error-message");
                            if (errorMessage.length > 0) {
                                console.dir(errorMessage);
                                $('#messages').html('<p class="msg msgError">' + errorMessage + '</p>');
                            } else {
                                $('#messages').html('<p class="msg msgError">' + "Трябва да се съгласите с условията" + '</p>');
                            }
                            isValid = false;
                        } else {
                            $(el).removeClass("validation-failed");
                            $(el).addClass("validation-passed");
                            $('#messages').html('');
                        }
                    }
                });
                return isValid;
            };

            $bullteineGdprCheck.on("change", validateGdpr);
            $newsletterForm.on('submit', function (e) {
                e.preventDefault();
                e.stopPropagation();
                $emailField = $('#stenik_newsletter_email');
                var allowAjax = true;
                email = $emailField.val();
                if (email == '') {
                    $emailField.parent().addClass('fieldError');
                    allowAjax = false;
                } else {
                    $emailField.parent().removeClass('fieldError');
                }
                if ($request && $request.readyState != 4) {
                    $request.abort();
                }
                if (!validateGdpr()) {
                    allowAjax = false;
                }
                if (!allowAjax) {
                    return;
                }
                $request = $.ajax({
                    url: '/bg/newsletter',
                    type: 'post',
                    data: $("#newsletter").serialize(),
                    dataType: 'json',
                    beforeSend: function () {
                        $("body").removeClass("loaded")
                    },
                    success: function (data) {
                        var msg = data.html;
                        if (data.success) {
                            $('#messages').html('<p class="msg msgSuccess">' + msg + '</p>');
                            var success = true;
                        } else {
                            $('#messages').html('<p class="msg msgError">' + msg + '</p>');
                        }
                    },
                    complete: function () {
                        $("body").addClass("loaded")
                    }
                });
            });
        });
    </script>
    <script>
        $(document).on('ready', function () {

            // sliderMain
            if ($('#sliderMain .swiper-slide').length > 1) {
                var sliderMain = new Swiper('#sliderMain', {
                    autoplay: 5000,
                    speed: 1000,
                    loop: true,
                    effect: 'fade',
                    prevButton: '#sliderPrev',
                    nextButton: '#sliderNext',
                    preventClicks: false,
                    bulletActiveClass: 'selected',
                    autoplayDisableOnInteraction: false,
                    keyboardControl: true,
                    mousewheelControl: false,
                    grabCursor: false,
                    spaceBetween: 0
                });

                // pause on hover
                $('#sliderMain').on('mouseenter', function () {
                    sliderMain.stopAutoplay()
                }).on('mouseleave', function () {
                    sliderMain.startAutoplay()
                });
            } else {
                $('#sliderPrev, #sliderNext').hide();
            };
        });
        $(window).load(function () {
            if ($(window).width() > 1024) {
                $('#sliderMain img').attr('src', $('#sliderMain img').attr('data-src'));
            }
        });
    </script>

    <script>
        $(document).ready(function () {
            function createPopup($item) {

                if ($item.length) {
                    var date = new Date();

                    if (localStorage.getItem('popupExpire')) {
                        var expirationTime = localStorage.getItem('popupExpire');
                        if (date.getTime() > expirationTime) {
                            $item.modal("show");
                            localStorage.setItem('popupExpire', (date.getTime() + ($item.data("expires") * 1000)));
                            var closeAfter = $item.data("closeafter");
                            if (closeAfter > 0) {
                                setTimeout(function () {
                                    $item.modal("hide");
                                }, closeAfter * 1000);
                            }
                        }

                    } else {
                        $item.modal("show");
                        localStorage.setItem('popupExpire', (date.getTime() + ($item.data("expires") * 1000)));
                        var closeAfter = $item.data("closeafter");
                        if (closeAfter > 0) {
                            setTimeout(function () {
                                $item.modal("hide");
                            }, closeAfter * 1000);
                        }
                    }

                }

            }
            createPopup($(".widget-popup"));
        })
    </script>
    <style>
        .modal-header .close {
            position: unset !important;
        }
    </style>

